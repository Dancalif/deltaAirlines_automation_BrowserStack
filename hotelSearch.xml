<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="HotelSearch Suite" parallel="tests" thread-count="1">

 <test name="HotelSearch Chrome Test">
 <parameter name="conf" value="conf"/>
  <parameter name="environment" value="Chrome"/>
  <parameter name="url" value="https://www.delta.com" />
  <parameter name ="daysFromCurrentDate" value="14" />
  <parameter name ="inputLocation" value="BOM" />
  <parameter name ="locationInDropdown" value="Mumbai, India, IN (BOM)" />
    <classes>
      <class name="com.deltaAirlines.delta_automation.HotelSearch_Test" />
    </classes>
  </test>
 <test name="HotelSearch Firefox Test">
  <parameter name="conf" value="conf"/>
  <parameter name="environment" value="Firefox"/>
  <parameter name="url" value="https://www.delta.com" />
  <parameter name ="daysFromCurrentDate" value="14" />
   <parameter name ="inputLocation" value="BOM" />
  <parameter name ="locationInDropdown" value="Mumbai, India, IN (BOM)" />
    <classes>
      <class name="com.deltaAirlines.delta_automation.HotelSearch_Test" />
    </classes>
  </test>
   <test name="HotelSearch IE Test">
  <parameter name="conf" value="conf"/>
  <parameter name="environment" value="IE"/>
  <parameter name="url" value="https://www.delta.com" />
  <parameter name ="daysFromCurrentDate" value="14" />
   <parameter name ="inputLocation" value="BOM" />
  <parameter name ="locationInDropdown" value="Mumbai, India, IN (BOM)" />
    <classes>
      <class name="com.deltaAirlines.delta_automation.HotelSearch_Test" />
    </classes>
  </test>

</suite>