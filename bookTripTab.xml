<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="BookTripTab Suite" parallel="tests" thread-count="1">
 
 <test name="BookTripTab Chrome Test">
 <parameter name="conf" value="conf"/>
  <parameter name="environment" value="Chrome"/>
  <parameter name="url" value="https://www.delta.com" />
  <parameter name ="stateWantedFrom" value="California" />
  <parameter name ="airportWantedFrom" value="SFO" />
   <parameter name ="countryWantedTo" value="India" />
  <parameter name ="airportWantedTo" value="BOM" />
  <parameter name ="daysFromCurrentDate" value="14" />
    <classes>
      <class name="com.deltaAirlines.delta_automation.BookTripTab_Test" />
    </classes>
  </test>
<test name="BookTripTab Firefox Test">
  <parameter name="conf" value="conf"/>
  <parameter name="environment" value="Firefox"/>
  <parameter name="url" value="https://www.delta.com" />
  <parameter name ="stateWantedFrom" value="California" />
  <parameter name ="airportWantedFrom" value="SFO" />
  <parameter name ="countryWantedTo" value="India" />
  <parameter name ="airportWantedTo" value="BOM" />
  <parameter name ="daysFromCurrentDate" value="20" />
    <classes>
      <class name="com.deltaAirlines.delta_automation.BookTripTab_Test" />
    </classes>
  </test>
  <test name="BookTripTab IE Test">
  <parameter name="conf" value="conf"/>
  <parameter name="environment" value="IE"/>
  <parameter name="url" value="https://www.delta.com" />
  <parameter name ="stateWantedFrom" value="California" />
  <parameter name ="airportWantedFrom" value="SFO" />
  <parameter name ="countryWantedTo" value="India" />
  <parameter name ="airportWantedTo" value="BOM" />
  <parameter name ="daysFromCurrentDate" value="20" />
    <classes>
      <class name="com.deltaAirlines.delta_automation.BookTripTab_Test" />
    </classes>
  </test>
</suite>